{
  "name": "vulpes/container",
  "type": "library",
  "description": "PHP FIG PSR-11 Container implementation.",
  "keywords": ["psr", "psr-11", "container", "dependency-injection", "autoload"],
  "license": "MIT",
  "authors": [
    {
      "name": "András Würth",
      "email": "wuertha@live.com"
    }
  ],
  "require": {
    "php": ">=8.1",
    "psr/container": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.1"
  },
  "autoload": {
    "psr-4": {
      "Container\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Container\\": "test/"
    }
  },
  "provide": {
    "psr/container-implementation": "2.0"
  }
}
